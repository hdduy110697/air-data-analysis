<mapper namespace="vn.com.irtech.sys.mapper.PermissionMapper">

    <!--Delete data in sys_role_permission table according to permissions or menu ID-->
    <delete id="deleteRolePermissionByPid">
        delete from sys_role_permission where pid = ${id}
    </delete>

</mapper>
